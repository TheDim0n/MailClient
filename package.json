{
  "name": "mail-client",
  "version": "0.0.0",
  "main": "dist/electron.ts",
  "scripts": {
    "config": "ts-node setEnv.ts",
    "start": "npm run config && ng serve",
    "build": "npm run config && ng build",
    "build-prod": "npm run config && ng build --configuration production",
    "build-electron" : "npm run build-prod && tsc --lib ES2018,DOM --target ES5 src/electron.ts --outDir dist",
    "start-electron": "electron .",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch": "ng build --watch --configuration development"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "dotenv": "^16.0.0",
    "electron": "^17.1.0",
    "ng-zorro-antd": "^13.1.0",
    "rxjs": "~7.5.0",
    "ts-node": "^10.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.5",
    "@angular/cli": "~13.2.5",
    "@angular/compiler-cli": "~13.2.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
}
